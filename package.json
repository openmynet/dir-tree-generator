{
  "name": "dir-tree-generator",
  "displayName": "dir tree generator",
  "description": "快速生成文件目录树的插件,使用.gitignore或.treeignore忽略目录 支持md文档折叠",
  "version": "1.0.1",
  "engines": {
    "vscode": "^1.10.0"
  },
  "publisher": "openmynet",
  "icon": "icon.png",
  "keywords": [
		"tree",
		"dir tree",
    "tree-generator",
    "树"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.generator"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.generator",
        "title": "复制目录树"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "explorerResourceIsFolder",
          "command": "extension.generator",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "build": "vsce package",
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./node_modules/mocha/bin/mocha"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.10.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/openmynet/dir-tree-generator"
  },
  "homepage": "https://github.com/openmynet/dir-tree-generator/README.md",
  "dependencies": {
    "ignore": "^5.1.8"
  }
}
